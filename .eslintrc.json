{
    "extends": [
        "airbnb"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "class-methods-use-this": 0,
        "no-unused-vars": ["error", { "varsIgnorePattern": "Tree|Track|Phone" }]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "Matreshka": true
    }
}
